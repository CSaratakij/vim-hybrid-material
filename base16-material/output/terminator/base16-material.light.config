[global_config]
  title_transmit_bg_color = "#cc6666"
  title_inactive_bg_color = "#707880"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#263238:#cc6666:#b5bd68:#f0c674:#81a2be:#b294bb:#8abeb7:#ecefef:#707880:#cc6666:#b5bd68:#f0c674:#81a2be:#b294bb:#8abeb7:#ffffff"
    foreground_color = "#37474f"
    background_color = "#ffffff"
    cursor_color = "#37474f"

  [[Base16 material Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#ffffff:#cc6666:#b5bd68:#f0c674:#81a2be:#b294bb:#8abeb7:#ecefef:#707880:#cc6666:#b5bd68:#f0c674:#81a2be:#b294bb:#8abeb7:#263238"
    foreground_color = "#37474f"
    background_color = "#ffffff"
    cursor_color = "#37474f"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 material Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
